pipeline {
    agent any

    tools {
        // Install the Maven version configured as "M3" and add it to the path.
        maven "maven"
    }

    stages {
        stage('Git Clone') {
            steps {
               git credentialsId: 'git', url: 'https://github.com/sandeepsingh07/hello-world-1.git'
            }
            }
        stage('Build'){
            steps{
                sh "mvn clean install"
            }
        }
        
    }
}
